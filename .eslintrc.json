{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console":"off",
        "linebreak-style":"off",
        "no-underscore-dangle":"off",
        "camelcase":"off",
        "import/no-extraneous-dependencies":"off",
        "no-undef":"off",
        "class-methods-use-this":"off",
        "no-unused-vars":"off",
        "operator-linebreak":"off",
        "object-shorthand":"off"
    }
}
